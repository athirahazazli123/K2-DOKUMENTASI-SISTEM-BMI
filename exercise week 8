#include <iostream> 
using namespace std; 
int main() { 
	int i, a[5]; 

		for (i = 0; i <= 4; i++) { 
			cout << "Enter 3 numbers for row " << i + 1 << " : "; 
			cin >> a[i]; cout << endl;
		}
		cout << "Elements of array are : " << endl; 
		for (i = 4; i >= 0; i--) { 
			cout << a[i];	
			cout << endl; 
		} 
		return 0;
}
